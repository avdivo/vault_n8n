# .env.example
# Переименуйте этот файл в .env и задайте свои значения

# Обязательный токен авторизации
AUTH_TOKEN=your_secret_token_here

# Обязательный ключ шифрования в виде 64-символьной hex-строки (32 байта)
# Сгенерировать можно командой: openssl rand -hex 32
ENCRYPTION_KEY=your_64_char_hex_encryption_key_here

# Путь к файлу базы данных (опционально, по умолчанию ./secrets.db)
# DATABASE_PATH=./secrets.db
